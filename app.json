{
  "expo": {
    "name": "Retronatus",
    "slug": "Retronatus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#232831"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/main-icon.png"
    }
  }
}
